stack = [] #создаём пустой стек
stack.append('A') #добавляет элементы в стек
stack.append('B')
stack.append('C')
stack.pop() #удаляет последний элемент из стека
print(stack) #вывод содержания стека


list = [1, 2, 3, 4] #создаём массив
list.append(5) #добавление элемента
list.remove(1) #удаление элемента по значению
print(list) #вывод содержания массива
